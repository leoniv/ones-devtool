// ШАБЛОННЫЕ ПЕРЕМЕННЫЕ
&НаКлиенте
Перем ИспользуемоеИмяФайла экспорт;
////////////////////////////////////////////////////
// ШАБЛОННЫЕ ФУНКЦИИ
&НаСервере
function Object()
  return РеквизитФормыВЗначение(""Объект"");	
endfunction	
&НаСервере
function GetLib(LibName)
  return Object().libs[LibName];	
endfunction	
&НаКлиенте
//Возвращает доп. форму текущей обработки
// Параметры
// - ИмяФормы - строка - имя формы обработки
function ПолучитьФормуОбработки(ИмяФормы
,Параметры=Неопределено
,Владелец=Неопределено
,Уникальность=Неопределено
,Окно=Неопределено
,НавигационнаяСсылка=Неопределено)
	Возврат ПолучитьФорму(""ВнешняяОбработка.""+ ПодключитьОбработку(ХранилищеОбработки())+"".Форма.""+ИмяФормы
,Параметры
,Владелец
,Уникальность
,Окно
,НавигационнаяСсылка);
КонецФункции	
&НаСервере
функция ПодключитьОбработку(ХранилищеТестов)
	ИмяОбработкиНаСервере = Строка(Новый УникальныйИдентификатор);
    ВнешниеОбработки.Подключить(ХранилищеТестов
,ИмяОбработкиНаСервере
,false);
    Возврат  ИмяОбработкиНаСервере;	
КонецФункции	
&Насервере
функция ИспользуемоеИмяФайла(ИспользуемоеИмяФайла)
	Если ИспользуемоеИмяФайла = Неопределено Тогда
	    Возврат Object().ИспользуемоеИмяФайла;
	Иначе
		Возврат  ИспользуемоеИмяФайла;
	КонецЕсли;
конецФункции
&НаКлиенте
function ХранилищеОбработки()
	Возврат ПоместитьВоВременноеХранилище(Новый ДвоичныеДанные(ИспользуемоеИмяФайла(ИспользуемоеИмяФайла)));
endfunction	
&НаКлиенте
Процедура reload(Команда)
	_Test_manage = ПолучитьФорму(""ВнешняяОбработка.""+ ПодключитьОбработку(ХранилищеОбработки())+"".Форма.Main""
,ПараметрыПерезапуска());
	_Test_manage.ИспользуемоеИмяФайла = ИспользуемоеИмяФайла(ИспользуемоеИмяФайла);
	_Test_manage.Открыть();
	Закрыть();
КонецПроцедуры
&НаКлиенте
function ПараметрыПерезапуска()
	raise ""TODO установите парамтры перезапуска""
endfunction	
